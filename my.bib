  author        = {Gevorkyan, Migran Nelsonovich and Korolkova, Anna Vladislavovna and Kulyabov, Dmitry Sergeevich},
  title         = {Overview of social networks research software},
  booktitle     = {Proceedings of the Selected Papers of the 9th International Conference "Information and Telecommunication Technologies and Mathematical Modeling of High-Tech Systems" (ITTMM-2019), Moscow, Russia, April 15-19, 2019},
  year          = {2019},
  editor        = {Kulyabov, Dmitry S. and Samouylov, Konstantin E. and Sevastianov, Leonid A.},
