}

@Article{kulyabov:2016:convergent:one-step-diagram,
  author        = {Еферина, Екатерина Геннадьевна and Велиева, Татьяна Рефатовна and Королькова, Анна Владиславовна and Гнатич, Михаил and Кулябов, Дмитрий Сергеевич and Севастьянов, Леонид Антонович},
  title         = {Диаграммное представление операторного формализма одношаговых процессов},
  journal       = {Современные информационные технологии и ИТ-образование},
  year          = {2016},
  language      = {russian},
  volume        = {12},
  number        = {1},
  pages         = {28--34},
  abstract      = {В развиваемом группой методе стохастизации одношаговых процессов из исходной стохастической системы получаются упрощённые математические модели. Эти модели возможно исследовать стандартными методами, в отличии от исходной системы. Однако сам процесс стохастизации зависит от типа исследуемой системы. Мы хотим получить унифицированный абстрактный формализм для стохастизации одношаговых процессов. Данный формализм должен быть эквивалентен ранее введёному. Для унификации методов построения основного кинетического уравнения предлагается использовать диаграммную технику. Предложена диаграмная техника, позволяющая получать унифицированным образом основное кинетическое уравнение для исследуемой системы.},
