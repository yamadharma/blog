  file          = {:/bib/2017/Kulyabov_2017_J._Phys.__Conf._Ser._788_012025.pdf:pdf},
  keywords      = {rfbr14-01-00628, rfbr15-07-08795, rfbr16-07-00556, rudn5-100, sjr-q3, wos, rinc, scopus},
  mendeley-tags = {Scopus,WoS,РИНЦ},
  publisher     = {IOP Publishing},
  timestamp     = {2020-03-30},
  type          = {Conference Paper},
}

