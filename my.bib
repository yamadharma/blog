}

@Article{kulyabov:2017:mmcs:functional_integrals,
  author        = {Ayrjan, Edik Artashevich and Egorov, A. D. and Kulyabov, Dmitry Sergeevich and Malyutin, V. B. and Sevastyanov, Leonid Antonovich},
  title         = {Application of Functional Integrals to Stochastic Equations},
  journal       = {Mathematical Models and Computer Simulations},
  year          = {2017},
